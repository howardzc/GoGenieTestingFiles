<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.howard.gogenie.Home"
    android:background="@drawable/homepage">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/engButton"
        android:src="@drawable/englishbutton"
        android:padding="1dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="120dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chiButton"
        android:src="@drawable/chinesebutton"
        android:padding="1dp"
        android:layout_marginTop="50dp"
        android:layout_alignTop="@+id/engButton"
        android:layout_alignLeft="@+id/engButton"
        android:layout_alignStart="@+id/engButton" />

</RelativeLayout>
